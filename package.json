{
  "name": "n8n-nodes-sat-mexico",
  "version": "0.1.1",
  "description": "Nodo n8n para integración con SAT México - Facturación CFDI, RFC, UDI, tipo de cambio",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "sat",
    "mexico",
    "rfc",
    "cfdi",
    "facturacion",
    "impuestos",
    "udi",
    "banxico",
    "latam"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Manuelreyesbravo/n8n-nodes-sat-mexico",
  "author": {
    "name": "Manuel Reyes Bravo",
    "email": "manuelreyesbravo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manuelreyesbravo/n8n-nodes-sat-mexico.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SatMexicoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SatMexico/SatMexico.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
